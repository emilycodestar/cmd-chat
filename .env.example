# CMD CHAT - Exemplo de Configuração de Ambiente

# Servidor
SERVER_HOST=0.0.0.0
SERVER_PORT=1000
ADMIN_PASSWORD=your_secure_password_here

# SSL/TLS (Opcional - para produção com HTTPS/WSS)
SSL_CERT_PATH=/path/to/cert.pem
SSL_KEY_PATH=/path/to/key.pem

# Tokens
TOKEN_TTL=86400  # Tempo de vida do token em segundos (padrão: 24 horas)

# Cliente
CLIENT_SERVER=localhost
CLIENT_PORT=1000
CLIENT_USERNAME=your_username
CLIENT_USE_SSL=false  # true para wss://, false para ws://
